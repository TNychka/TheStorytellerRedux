            </rulesStrings>
        </questDescriptionRules>
        <root Class="BST_TheStorytellerRedux.QuestNode_Root_TheStorytellersTale">
            <rootNodes>
                <li Class="QuestNode_QuestUnique">
                    <tag>BSTStorytellerTale</tag>
                </li>
                <li Class="QuestNode_GetPlayerFaction">
                    <storeAs>playerFaction</storeAs>
                </li>
            </rootNodes>
            <introNode>
                <li Class="BST_TheStorytellerRedux.QuestPart_Arc_Establishment_TheStorytellerRedux">
                </li>
            </introNode>
            <arcNodes>
               <li Class="BST_TheStorytellerRedux.QuestPart_Arc_Survival_TheStorytellerRedux">
               </li>
            </arcNodes>
        </root>
